# Frontend Environment Configuration Template
# Copy this file to .env and fill in actual values

# Application
VITE_APP_NAME=Secure Encryption Dashboard
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_AUDIT_LOGS=true
VITE_ENABLE_MFA=false
VITE_ENABLE_FIELD_LEVEL_ACCESS=false

# UI Configuration
VITE_ITEMS_PER_PAGE=20
VITE_AUTO_MASK_TIMEOUT=300000
VITE_REVEAL_TOKEN_EXPIRY=300000

# Authentication
VITE_SESSION_TIMEOUT=3600000
VITE_TOKEN_REFRESH_INTERVAL=3540000

# Monitoring & Analytics
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# Development
VITE_LOG_LEVEL=info
VITE_DEBUG_MODE=false
